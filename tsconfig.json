{
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./types" 
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./build",
    "module": "commonjs",
    "target": "es6"
  },
  "include": ["src/**/*", "seed.ts"]
}
