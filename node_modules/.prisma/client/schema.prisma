datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                    Int       @id @unique @default(autoincrement())
  name                  String
  email                 String    @unique
  phone                 String
  positionId            Int
  position              Positions @relation(fields: [positionId], references: [id])
  photo                 String
  registrationTimestamp Int       @default(0)
}

model Positions {
  id   Int    @id @unique @default(autoincrement())
  name String

  users User[]
}
